{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "cinemarathons/marathon",
	"version": "1.0.0",
	"title": "Cinemarathon",
	"category": "cinemarathons",
	"description": "Displays a list of movies you plan to watch along with a few options to add more fun to the fun.",
	"attributes": {
		"id": {
			"type": "integer"
		},
		"anchor": {
			"type": "string"
		},
		"title": {
			"type": "string",
			"default": ""
		},
		"image": {
			"type": "integer",
			"default": 0
		},
		"description": {
			"type": "string",
			"default": ""
		},
		"objectives": {
			"type": "string",
			"default": ""
		},
		"comments": {
			"type": "string",
			"default": ""
		},
		"movies": {
			"type": "array",
			"default": []
		},
		"expertMode": {
			"type": "boolean",
			"default": false
		}
	},
	"supports": {
		"anchor": true,
		"html": false
	},
	"textdomain": "cinemarathons",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"render": "file:./render.php"
}
